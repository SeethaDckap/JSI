                                  sortOrder="140"/>
                    </resource>
                    <resource id="Epicor_Customer::my_account_returns" title="My Returns" translate="title"
                              sortOrder="210"
                              displaytype="grid">
                        <resource id="Epicor_Customer::my_account_returns_read" actioncode="read" title="Read"
                                  sortOrder="100"/>
                        <resource id="Epicor_Customer::my_account_returns_details" actioncode="details" title="Details"
                                  sortOrder="110"/>
                        <resource id="Epicor_Customer::my_account_returns_create" actioncode="create" title="Create"
                                  sortOrder="120"/>
                        <resource id="Epicor_Customer::my_account_returns_edit" actioncode="edit" title="Edit"
                                  sortOrder="130"/>

                                  sortOrder="130"/>
                    </resource>
                    <resource id="Epicor_Customer::my_account_quotes" title="My Quotes" translate="title" sortOrder="170"
                              displaytype="grid">
                        <resource id="Epicor_Customer::my_account_quotes_read" actioncode="read" title="Read"

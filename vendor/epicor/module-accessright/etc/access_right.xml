                        <resource id="Epicor_Customer::my_account_dashboard_newsletters" title="Newsletters"
                                  translate="title" sortOrder="110" displaytype="grid">
                            <resource id="Epicor_Customer::my_account_dashboard_newsletters_edit" actioncode="edit" title="Edit"
                                      sortOrder="110"/>
                        </resource>
                    </resource>
                    <resource id="Epicor_Customer::my_account_approvals_dashboard" title="Order Approvals Dashboard"
                              sortOrder="115"/>

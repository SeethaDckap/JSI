                        <resource id="Epicor_Checkout::checkout_checkout_can_checkout" actioncode="can_checkout " title="Can Checkout "
                                  sortOrder="100"/>
                        <resource id="Epicor_Checkout::checkout_checkout_cart_list" actioncode="cart_list" title="Save Cart As List"
                                  sortOrder="110"/>
                        <resource id="Epicor_Checkout::checkout_checkout_cart_quote" actioncode="cart_quote" title="Convert To Quotation"
                                  sortOrder="120"/>
                        <resource id="Epicor_Checkout::checkout_checkout_multi_address" actioncode="multi_address" title="Checkout with Multiple Addresses"
                                  sortOrder="130"/>
                    </resource>
                    <resource id="Epicor_Checkout::checkout_account_summary" title="Account Summary" translate="title" sortOrder="120" />
                    <resource id="Epicor_Checkout::checkout_quick_order_pad" title="Quick Order Pad" translate="title" sortOrder="130" />

                                  sortOrder="100"/>
                    </resource>
                    <resource id="Epicor_Customer::my_account_address_book" title="Address Book"
                              sortOrder="160" displaytype="grid">
                        <resource id="Epicor_Customer::my_account_address_book_read" actioncode="read" title="Read"
                                  sortOrder="100"/>
                        <resource id="Epicor_Customer::my_account_address_book_create" actioncode="create" title="Create"
                                  sortOrder="110"/>
                        <resource id="Epicor_Customer::my_account_address_book_edit" actioncode="edit" title="Edit"
                                  sortOrder="120"/>

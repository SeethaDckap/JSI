                                  sortOrder="120"/>
                        <resource id="Epicor_Customer::my_account_quotes_reject" actioncode="reject" title="Reject"
                                  sortOrder="130"/>
                        <resource id="Epicor_Customer::my_account_quotes_details" actioncode="details" title="Details"
                                  sortOrder="140"/>
                    </resource>
                    <resource id="Epicor_Customer::my_account_information" title="Account Information OR Update Contact Information"
                              sortOrder="180"/>
                    <resource id="Epicor_Customer::my_account_ar_payment_received" title="My AR Payments Received"
                              translate="title" sortOrder="190" displaytype="grid">
                            <resource id="Epicor_Customer::my_account_ar_payment_received_read" actioncode="read" title="Read"
                                      sortOrder="100"/>
                            <resource id="Epicor_Customer::my_account_ar_payment_received_details" actioncode="details" title="Details"
                                      sortOrder="110"/>

                    <resource id="Epicor_Customer::my_account_wishlist" title="My Wishlist"
                              sortOrder="140"/>
                    <resource id="Epicor_Customer::my_account_stored_payment_methods" title="Stored Payment Methods" translate="title"
                              sortOrder="150" displaytype="grid">

<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Epicor_AccessRight:etc/access_right.xsd">
    <acl>
        <resources>
            <resource id="Magento_Frontend::all" title="All">
                <resource id="Epicor_Checkout::checkout" title="Checkout" translate="title"
                          actioncontoler="true" sortOrder="200">
                    

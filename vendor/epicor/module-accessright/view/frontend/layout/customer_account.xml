                    <argument name="resource" xsi:type="string">Epicor_Customer::my_account_dashboard</argument>
                </arguments>
            </referenceBlock>
            <referenceBlock  name="customer-account-navigation-orders-link">
                <arguments>
                    <argument name="resource" xsi:type="string">Epicor_Customer::my_account_orders</argument>
                </arguments>
            </referenceBlock>
            <referenceBlock  name="customer-account-navigation-account-edit-link">

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/configuratorCheck" method="POST">
        <service class="Epicor\Comm\Api\ConfiguratorCheckInterface" method="configuratorCheck"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/addToWishlist" method="POST">
        <service class="Epicor\Comm\Api\AddToWishlistInterface" method="addToWishlist"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <update handle="customer_account"/>
        <update handle="customer_returns_details"/>
    <head>
        <title>Customer Return Details</title>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Epicor\Comm\Block\Customer\Returns\Details\Title" name="returns.title"/>
        </referenceContainer>
        <referenceBlock name="customer.account.link.back">
            <action method="setRefererUrl">
                <argument name="value" xsi:type="string">/comm/returns/lists/</argument>
            </action>
            <action method="setListUrl">
                <argument name="value" xsi:type="string">/comm/returns/lists/</argument>
            </action>
            <action method="setListType">
                <argument name="value" xsi:type="string">Returns</argument>
            </action>
        </referenceBlock>
    </body>
</page>

    </type>
    <type name="Epicor\Lists\Controller\Adminhtml\Epicorlists\Lists\Save">
        <arguments>
            <argument name="listProductPositionFactory" xsi:type="object">Epicor\Lists\Model\ResourceModel\ListProductPositionFactory</argument>
        </arguments>

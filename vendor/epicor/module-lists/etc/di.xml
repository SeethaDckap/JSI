    <type name="Epicor\Lists\Block\Customer\Account\Listing\Renderer\Position">
        <arguments>
            <argument name="products" xsi:type="object">Epicor\Lists\Block\Adminhtml\Listing\Edit\Tab\Products</argument>
        </arguments>
